<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>名前</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
   <% members.each do |member| %>
    <tr>
     <td><%= attachment_image_tag(member, :profile_image, :fill, 50, 50, fallback: "no-image.jpg") %></td>
     <td><%= member.name %></td>
     <td><%= link_to 'Show', member_path(member), class: "member_#{member.id}" %></td>
    </tr>
   <% end %>
  </tbody>
</table>